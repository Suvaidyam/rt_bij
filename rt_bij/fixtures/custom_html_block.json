  "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Bootstrap Example</title>\n    <link href=\"https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css\" rel=\"stylesheet\">\n</head>\n<body>\n    <div class=\"container-fluid d-flex justify-content-center \">\n        <div class=\" col bg-white p-4 rounded-lg shadow-sm\">\n            <h2 class=\"text-lg font-semibold  mb-4\">\n                CRS4D: Climate Resilience for Sustainable Development\n            </h2>\n            <div class=\"row \">\n                <div class=\"col-6 mb-3\">\n                    <div class=\"bg-info card1 p-3 rounded-lg\">\n                        <p>Diversification of Food Basket</p>\n                        <p class=\"font-semibold\">600</p>\n                    </div>\n                </div>\n                <div class=\"col-6\">\n                    <div class=\"bg-warning card2 p-3 rounded-lg\">\n                        <p>Water Harvesting Capacity Created</p>\n                        <p class=\"font-semibold\">600</p>\n                    </div>\n                </div>\n                <div class=\"col-6 mb-3 \">\n                    <div class=\"bg-secondary text-white card3 p-3 rounded-lg\">\n                        <p>Drinking Water Capacity Created</p>\n                        <p class=\"font-semibold\">600</p>\n                    </div>\n                </div>\n                <div class=\"col-6 mb-3\">\n                    <div class=\"bg-danger card4 p-3 rounded-lg\">\n                        <p>Crop Water Budgeting</p>\n                        <p class=\"font-semibold\">600</p>\n                    </div>\n                </div>\n                <div class=\"col-12\">\n                    <div class=\"bg-success p-3 card5 rounded-lg\">\n                        <p>Grey Water Treatment(Water Recycle)</p>\n                        <p class=\"font-semibold\">600</p>\n                    </div>\n                </div>\n            </div>\n        </div>\n\n        <div class=\" col bg-white ml-4 p-4 rounded-lg shadow-sm \">\n            <h2 class=\"text-lg font-semibold text-dark mb-4\">\n                Integrated Value Chain Development (IVCD)\n            </h2>\n            <div class=\"row \">\n                <div class=\"col-6\">\n                    <div class=\"bg-warning card6 p-3  text-white rounded-lg\">\n                        <p>No. of producers</p>\n                        <p class=\"font-semibold\">600</p>\n                    </div>\n                </div>\n                <div class=\"col-6 mb-3\">\n                    <div class=\"bg-success card7 p-3 rounded-lg\">\n                        <p>Value of transaction</p>\n                        <p class=\"font-semibold\">600</p>\n                    </div>\n                </div>\n                <div class=\"col-12 \">\n                     <div class=\"bg-secondary card8 p-3 rounded-lg\">\n                        <p>Value of transaction</p>\n                        <p class=\"font-semibold\">600</p>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <script src=\"https://code.jquery.com/jquery-3.3.1.slim.min.js\"></script>\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js\"></script>\n    <script src=\"https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js\"></script>\n</body>\n</html>",
